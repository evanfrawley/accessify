{
  "manifest_version": 2,
  "name": "Accessify",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.js", "content.js"]
    }
  ],
  "browser_action": {
	  "default_icon": "icon.png"
	},
	"background": {
	  "scripts": ["background.js"]
	},
	"web_accessible_resources": ["styles.css"]
}